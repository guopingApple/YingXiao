/* partner page */
.partners{ min-width: 1200px; padding: 0 9%;}
.align-content{text-align: center;-webkit-align-content:flex-start;-moz-align-content:flex-start;-ms-align-content:flex-start;align-content: flex-start;-webkit-flex-wrap: wrap;-moz-flex-wrap: wrap;-ms-flex-wrap: wrap;flex-wrap: wrap;}
