.apply{width: 1200px;margin: 0 auto;}
.apply .red{display: block;height: 50px;line-height: 50px;}
.apply a.submit{display:block;width: 390px;height: 67px;line-height: 67px; margin:0 auto;background-color: #4b93f5;font-size:28px;color: #fff;margin-bottom: 50px;}
.apply input,.apply select,.apply textarea{border: solid 1px #bcbcbc;outline:medium;color: #333;font-size: 18px;margin-bottom: 20px;}
.apply textarea{width: 1190px;height: 130px;line-height: 20px;}
.apply input.tel,.apply .compName{width: 570px;height: 50px;line-height: 50px;}
.apply .name{width: 275px;height: 50px;line-height: 50px;}
.radioIcon{width: 26px;height: 26px;background-color: #f0f0f0;border: solid 1px #bababa;-webkit-border-radius: 50%;-moz-border-radius: 50%;-ms-border-radius: 50%;-o-border-radius: 50%; border-radius: 50%; display: inline-block;vertical-align: middle;margin-right: 20px;position: relative;top: -2px;position: relative;}
.radioSelect{width: 10px;height: 10px;display: block; background-color: #9d9d9d;-webkit-border-radius: 50%;-moz-border-radius: 50%;-ms-border-radius: 50%;-o-border-radius: 50%; border-radius: 50%;position: absolute;top: 50%;left: 50%;-webkit-transform: translate(-50%,-50%);-moz-transform: translate(-50%,-50%);transform: translate(-50%,-50%);}
.iwant em,.gender em{display: none;}
/* layer */
.hot_product_layer{width: 1196px;height: 610px;padding: 104px 0 0 90px;background-color: #fff;position: absolute;top: 50%;left: 50%; margin: -354px 0 0 -650px; z-index: 9999; display: none;border: solid 1px #d3d3d3;-webkit-box-shadow: 0 0 15px 5px #d3d3d3;-moz-box-shadow: 0 0 15px 5px #d3d3d3;-ms-box-shadow: 0 0 15px 5px #d3d3d3;box-shadow: 0 0 15px 5px #d3d3d3;}
.hot_layer_b{width: 100%; height: 87px;border-top: solid 1px #e1e1e1;position: absolute;left: 0;right: 0;bottom: 0;}
.hot_layer_b a{width: 87px;height: 87px;display:block;position: absolute;bottom: 0;background: url(../img/hot_arrow.png) no-repeat center;}
.hot_layer_b a.lar{left: 65px;/*-webkit-transform: rotateY(-180deg);-moz-transform: rotateY(-180deg);-ms-transform: rotateY(-180deg); transform: rotateY(-180deg);*/background: url(../img/hot_arrow_left.png) no-repeat center;}
.hot_layer_b a.rar{right: 65px;}
.hot_layer_close{width: 66px;height: 66px;position: absolute;top: 0;right: 28px;background: url(../img/hot_close.png) no-repeat center;}
.products{width: 1115px;height: 455px;overflow: hidden;position: relative;}
.products ul{position: absolute;left: 0;top: 0;}
.products ul li{width: 260px;height: 545px;float: left;margin-right: 25px;position: relative;cursor: pointer;}
.products ul li>img{width: 260px;height: 454px;}
.products ul li>div{position: absolute;background:#001f40;background: rgba(0,31,64,0.55);text-align:center;width:260px;height: 454px;z-index: 99;top: 0;left: 0;color: #fff;font-size: 16px;padding-top: 128px; display: none;}
.products ul li>div h3{font-size: 20px;line-height: 22px;}
.products ul li.hover div{display: block;}
.products ul li.leave div{display: none;}

